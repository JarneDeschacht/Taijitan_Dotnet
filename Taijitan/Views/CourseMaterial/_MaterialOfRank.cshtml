@model IEnumerable<Taijitan.Models.Domain.CourseMaterial>
@{
    ViewData["Title"] = "_MaterialOfRank";
}




<div class="col-lg-12">
    @{
        foreach (var mat in Model)
        {
            string fotolink = String.Format("{0}.jpg", mat.Images.First().Name);
            <div class="col-lg-4 thumbnail">
                <img class="img-thumbnail rounded mx-auto d-block" src="~/images/@fotolink" />
                <div class="card-body">
                    <h5>@mat.Title</h5>
                    <a href="#" class="btn btn-primary">Bekijk</a>
                </div>
            </div>
        }
    }
</div>