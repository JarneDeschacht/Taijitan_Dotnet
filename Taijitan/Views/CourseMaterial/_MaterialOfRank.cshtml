@model IEnumerable<Taijitan.Models.Domain.CourseMaterial>
@{
    ViewData["Title"] = "_MaterialOfRank";
}




<div class="col-lg-12">
    @{
        foreach (var mat in Model)
        {
           <div class="card">
               <img class="card-img-top" src="~/images/@mat.Images.First()"/>
               <div class="card-body">
                   <h5 class="card-title">@mat.Title</h5>
                   <a href="#" class="btn btn-primary">Bekijk</a>
               </div>
           </div>
        }
    }
</div>